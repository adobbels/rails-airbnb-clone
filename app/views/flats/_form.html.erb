<%= simple_form_for(@flat) do |f| %>
  <%= f.input :name %>
  <%= f.input :description %>
  <%= f.input :category %>
  <%= f.input :price %>
  <%= f.input :capacity %>
  <%= f.input :address %>
  <%= f.input :post_code %>
  <%= f.input :city %>
  <%= f.input :photo %>
  <%= f.input :photo_cache, as: :hidden %>
  <%= f.submit %>
<% end %>

<%= content_for(:after_js) do %>
  <script src="https://maps.googleapis.com/maps/api/js?libraries=places&amp;key=AIzaSyCwTzvVbIussroQDL3o9_DNDNViQzTPEdc"></script>
<% end %>


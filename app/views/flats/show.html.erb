<%= render "shared/banner_flat" %>
<%= render "shared/formulaire_flat" %>

<div class="containe">
  <div class="row">
    <div class="col-xs-offset-6 col-xs-6">
      <p><%= @flat.capacity %> persons</p>
      <!-- <p><%= cl_image_tag @flat.photo, height: 169, width: 354, crop: :fill %></p> -->
      <p>Formulaire</p>
      <p>Number of people</p>
      <p>Start Date</p>
      <p>End Date</p>
      <p><%= @flat.description %></p>
      <p>Equipements</p>
      <p>Reviews</p>
    </div>
  </div>
</div>


<% content_for(:after_js) do %>
<%= javascript_tag do %>
  const coord = <%= raw @flat_coordinates.to_json %>;
    alert('Flat coordinates are ' + coord.lat + ', ' + coord.lng);
  <% end %>
<% end %>

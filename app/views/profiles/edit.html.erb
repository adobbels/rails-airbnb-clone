<div class="container_login_long banner-flat" style="background-image: url('http://res.cloudinary.com/dwkqph9as/image/upload/v1511970291/BannerImage_vp7iqa.jpg');">
  <div class="form_card">
    <div class="zone_form">
      <%= simple_form_for [@profile] do |f| %>
        <%= f.input :first_name %>
        <%= f.input :last_name %>
        <%= f.input :address %>
        <%= f.input :city %>
        <%= f.input :post_code %>
        <%= f.input :phone_number %>
        <%= f.input :sex %>
        <%= f.input :birth_date %>
        <%= f.input :photo %>
        <%= f.input :photo_cache, as: :hidden %>
        <%= f.button :submit, class:"btn" %>
      <% end %>
    </div>
  </div>
</div>
<%= render "shared/footer" %>

<%= simple_form_for [current_user, @profile] do |f| %>
  <%= f.input :first_name %>
  <%= f.input :last_name %>
  <%= f.input :address %>
  <%= f.input :city %>
  <%= f.input :post_code %>
  <%= f.input :phone_number %>
  <%= f.input :sex %>
  <%= f.input :birth_date %>
  <%= f.button :submit, class:"btn btn-primary" %>
<% end %>

